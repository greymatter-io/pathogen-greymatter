{
    "zone_key": "{{ variable "zone" }}",
    "listener_key": "listener-{{ variable "serviceName" }}-{{ variable "redisServiceName" }}-egress",
    "domain_keys": [
        "domain-{{ variable "serviceName" }}-{{ variable "redisServiceName" }}-egress"
    ],
    "name": "egress",
    "ip": "0.0.0.0",
    "port": {{ variable "redisEgressPort" }},
    "protocol": "http_auto",
    "tracing_config": null,
    "active_network_filters": ["envoy.tcp_proxy"],
    "network_filters": {
        "envoy_tcp_proxy": {
            "stat_prefix": "egress_redis",
            "cluster": "{{ variable "redisServiceName" }}"
          }
    }
}
