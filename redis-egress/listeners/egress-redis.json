{
    "zone_key": "{{ variable "zone" }}",
    "listener_key": "listener-{{ variable "serviceName" }}-{{ variable "redisServiceName" }}-egress",
    "domain_keys": [
        "domain-{{ variable "serviceName" }}-{{ variable "redisServiceName" }}-egress"
    ],
    "name": "egress",
    "ip": "0.0.0.0",
    "port": {{ variable "redisEgressPort" }},
    "protocol": "http_auto",
    "tracing_config": null,
    "active_network_filters": "envoy.redis_proxy",
    "network_filters": {
        "envoy_redis_proxy": {
            "stat_prefix": "redis-egress",
            "settings": {
                "op_timeout": "10000s"
            },
            "prefix_routes": {
                "catch_all_route": {
                    "cluster": "{{ variable "serviceName" }}-{{ variable "redisServiceName" }}-cluster"
                }
            }
        }
    }
}
