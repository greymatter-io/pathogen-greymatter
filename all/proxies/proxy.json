{
    "zone_key": "{{ variable "zone" }}",
    "proxy_key": "{{ variable "serviceName" }}-proxy",
    "domain_keys": [
        "domain-{{ variable "serviceName" }}-ingress",
        "domain-{{ variable "serviceName" }}-egress"
    ],
    "listener_keys": [
        "listener-{{ variable "serviceName" }}-ingress",
        "listener-{{ variable "serviceName" }}-egress"
    ],
    "name": "{{ variable "serviceName" }}",
    "listeners": null,
    "active_proxy_filters": [
        "gm.metrics"
    ],
    "proxy_filters": {
        "gm_metrics": {
            "metrics_port": 8080,
            "metrics_host": "0.0.0.0",
            "metrics_dashboard_uri_path": "/metrics",
            "metrics_prometheus_uri_path": "/prometheus",
            "metrics_ring_buffer_size": 4096,
            "prometheus_system_metrics_interval_seconds": 15,
            "metrics_key_function": "depth"
        }
    }
}
